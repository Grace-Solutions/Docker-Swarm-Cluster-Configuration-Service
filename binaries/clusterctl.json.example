{
  "globalSettings": {
    "clusterName": "docker-swarm-0001",
    "overlayProvider": "netbird",
    "overlayConfig": "your-netbird-setup-key-here --allow-server-ssh --enable-ssh-remote-port-forwarding --enable-ssh-sftp --enable-ssh-root",
    "glusterVolume": "docker-swarm-0001",
    "glusterMount": "/mnt/GlusterFS/docker-swarm-0001/data",
    "glusterBrick": "/mnt/GlusterFS/docker-swarm-0001/brick",
    "glusterDiskManagement": false,
    "setRootPassword": "",
    "servicesDir": "",
    "removeSSHPublicKeyOnCompletion": false,
    "preScripts": [
      {
        "enabled": true,
        "name": "pre-deployment-check",
        "source": "https://example.com/scripts/pre-check.sh",
        "parameters": "--verbose",
        "conditions": []
      },
      {
        "enabled": true,
        "name": "worker-specific-setup",
        "source": "https://example.com/scripts/worker-setup.sh",
        "parameters": "",
        "conditions": [
          { "property": "role", "operator": "=", "value": "worker", "negate": false }
        ]
      },
      {
        "enabled": true,
        "name": "manager-specific-setup",
        "source": "https://example.com/scripts/manager-setup.sh",
        "parameters": "",
        "conditions": [
          { "property": "role", "operator": "=", "value": "manager", "negate": false }
        ]
      },
      {
        "enabled": true,
        "name": "non-manager-nodes",
        "source": "https://example.com/scripts/non-manager-setup.sh",
        "parameters": "",
        "conditions": [
          { "property": "role", "operator": "=", "value": "manager", "negate": true }
        ]
      },
      {
        "enabled": true,
        "name": "production-nodes-only",
        "source": "https://example.com/scripts/prod-setup.sh",
        "parameters": "",
        "conditions": [
          { "property": "label.environment", "operator": "=", "value": "production", "negate": false }
        ]
      },
      {
        "enabled": true,
        "name": "vps-nodes-regex",
        "source": "https://example.com/scripts/vps-setup.sh",
        "parameters": "",
        "conditions": [
          { "property": "hostname", "operator": "regex", "value": "^vps-.*", "negate": false }
        ]
      },
      {
        "enabled": true,
        "name": "non-vps-nodes",
        "source": "https://example.com/scripts/non-vps-setup.sh",
        "parameters": "",
        "conditions": [
          { "property": "hostname", "operator": "regex", "value": "^vps-.*", "negate": true }
        ]
      }
    ],
    "postScripts": [
      {
        "enabled": true,
        "name": "post-deployment-validation",
        "source": "https://example.com/scripts/post-validate.sh",
        "parameters": "--verbose",
        "conditions": []
      },
      {
        "enabled": true,
        "name": "worker-post-validation",
        "source": "https://example.com/scripts/worker-validate.sh",
        "parameters": "",
        "conditions": [
          { "property": "role", "operator": "=", "value": "worker", "negate": false },
          { "property": "glusterEnabled", "operator": "=", "value": "true", "negate": false }
        ]
      }
    ]
  },
